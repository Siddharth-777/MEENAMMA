{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/", "destination": "/templates/index.html" },
    { "source": "/gallery", "destination": "/templates/gallery.html" }
  ],
  "static": {
    "headers": [
      {
        "source": "**/*.@(js|css|woff|gif|jpg|png)",
        "headers": [
          { "key": "Cache-Control", "value": "max-age=31536000" }
        ]
      }
    ]
  }
}
